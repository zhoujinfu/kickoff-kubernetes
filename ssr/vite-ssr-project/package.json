{
  "scripts": {
    "idl": ". proto/gentypes.sh && . proto_web/gen.sh",
    "dev": "concurrently --prefix \"{name}-{pid}\" --names \" MOCK,  DEV, ENVOY\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen\" \"pnpm run mock\" \"pnpm run server\" \"pnpm run envoy\"",
    "envoy": "envoy -c ./proto_web/envoy.yaml",
    "mock": "nodemon --watch ./mock ./mock/server.ts",
    "prod": "pnpm run build && pnpm run server:prod",
    "build": "vite build",
    "server": "ts-node ./server/index.ts",
    "server:prod": "cross-env NODE_ENV=production ts-node ./server/index.ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.13",
    "@grpc/proto-loader": "^0.7.6",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/google-protobuf": "^3.15.6",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/server-renderer": "^3.2.33",
    "compression": "^1.7.4",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.15",
    "express": "^4.18.1",
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.4.2",
    "node-fetch": "^3.3.1",
    "nodemon": "^2.0.22",
    "sirv": "^2.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-cp": "^1.0.0",
    "vite-plugin-inspect": "^0.7.18",
    "vite-plugin-ssr": "^0.4.110",
    "vue": "^3.2.33"
  },
  "type": "module"
}
